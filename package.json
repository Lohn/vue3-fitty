{
  "name": "vue3-fitty",
  "version": "0.1.0",
  "main": "./dist/vue-fitty.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "service-help": "vue-cli-service help",
    "inspect": "vue-cli-service inspect",
    "build": "vue-cli-service build --target lib --name vue3-fitty ./src/components/index.ts && cp index.d.ts dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lohn/vue3-fitty.git"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "fitty": "^2.3.3",
    "uuid": "^8.3.2",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "prettier": "^2.2.1",
    "typescript": "~4.1.5"
  },
  "description": "Fitty text wrapper for Vue 3",
  "bugs": {
    "url": "https://github.com/lohn/vue3-fitty/issues"
  },
  "homepage": "https://github.com/lohn/vue3-fitty#readme",
  "keywords": [
    "vue",
    "vue3",
    "fitty",
    "styling",
    "autosize",
    "resize",
    "font"
  ],
  "author": "Josemar Lohn <j@lo.hn>",
  "license": "MIT"
}